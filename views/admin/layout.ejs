<body class="sidebar-wide">

	<!-- Navbar -->
	<div class="navbar navbar-inverse" role="navigation">
		<div class="navbar-header">
			<a class="navbar-brand" href="/developers">
				<% if(!('owner_org' in config.get('app'))){ %>
					<img src="<%=config.get('app.cdn_url')%>/images/logo9.png">
				<% }else{ %>
					<img src="<%=config.get('app.org_logo')%>"><img src="<%=config.get('app.cdn_url')%>/images/powered-by-codemill-inside.png">
				<% } %>
			</a>
			<a class="sidebar-toggle"><i class="icon-paragraph-justify2"></i></a>
			<button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-icons">
				<span class="sr-only">Toggle navbar</span>
				<i class="icon-grid3"></i>
			</button>
			<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".sidebar">
				<span class="sr-only">Toggle navigation</span>
				<i class="icon-paragraph-justify2"></i>
			</button>
		</div>

		<ul class="nav navbar-nav navbar-right collapse" id="navbar-icons">
			<li class="user dropdown">
				<a class="dropdown-toggle" data-toggle="dropdown">
					<img src="">
					<span>Admin</span>
					<i class="caret"></i>
				</a>
				<ul class="dropdown-menu dropdown-menu-right icons-right">
					<% if(!('owner_org' in config.get('app'))){ %>
						<li><a href="/clients"><i class="icon-coin"></i> Switch to Clients Dashboard</a></li>
					<% } %>
					<li><a href="/logout"><i class="icon-exit"></i> Logout</a></li>
				</ul>
			</li>
		</ul>
	</div>
	<!-- /navbar -->


	<!-- Page container -->
 	<div class="page-container">


		<!-- Sidebar -->
		<div class="sidebar collapse">
			<div class="sidebar-content">



				<!-- Main navigation -->
				<ul class="navigation">
					<li class="<%= (active_page == 'spots' ? 'active' : '')%>"><a href="/admin/spots"><span>Spots</span> <i class="icon-coin"></i></a></li>
          <li class="<%= (active_page == 'clasify' ? 'active' : '')%>"><a href="/admin/classify"><span>Classify</span> <i class="icon-coin"></i></a></li>
				</ul>
				<!-- /main navigation -->

			</div>
		</div>
		<!-- /sidebar -->


		<!-- Page content -->
	 	<div class="page-content">


			<% if(alert){ %>
				<div class="page-header">
					<div class="page-title">
						&nbsp;
					</div>
				</div>
				<div class="panel-body">
					<div class="alert alert-block alert-<%=alert.type%> fade in block-inner">
						<button type="button" class="close" data-dismiss="alert" aria-hidden="true">Ã—</button>
						<i class="<%=alertIcons.getIcon(alert.type)%>"></i> <%-alert.message%>
					</div>
				</div>
			<% }%>


			<%-body -%>


	        <!-- Footer -->
	        <div class="footer clearfix">
		        <div class="pull-left">&copy; 2017. SwellPhone 3.0</div>
	        	<div class="pull-right icons-group">
	        		<a href="mailto:support@codemill.io"><i class="icon-envelop"></i></a>
	        		<a href="https://github.com/CodeMillApp/support/issues"><i class="icon-github3"></i></a>
	        		<a href="https://gitter.im/CodeMillApp/support"><img class="gitter-icon" src="<%=config.get('app.cdn_url')%>/images/gitter.png"></a>
	        	</div>
	        </div>
	        <!-- /footer -->


		</div>
		<!-- /page content -->


	</div>
	<!-- /page container -->

	<!-- Google Analytics -->
    <% if(app.get('env') == 'production'){%>
	     <% }%>
	<!-- /Google Analytics -->

