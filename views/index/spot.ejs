<section class="inner-section">
	<div class="container">
		<div class="row">
			<div class="col-md-6 col-md-offset-3">

        <% if(pics.length == 0){%>
          <h3>sorry, nothing to show</h3>
        <% }else{ %>
          <% pics.forEach(function(pic){ %>
            <div>
              <img src="<%=pic.url%>"/>
              <p>taken <%=moment(pic.date_taken).calendar(null,{sameElse:'DD-MM-YYYY'})%> by <a href="http://instagram.com/<%=pic.instagram.user.username%>">@<%=pic.instagram.user.username%></a></p>
            </div>
          <% }) %>
        <% } %>

			</div>
		</div>
	</div>
</section>
